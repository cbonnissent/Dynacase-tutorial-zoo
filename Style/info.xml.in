<?xml version="1.0"?>
<module name="@PACKAGE@-style" disabled="no" version="@VERSION@" release="@RELEASE@">

  <description>Zoo blue style.</description>

  <requires>
    <module name="dynacase-core" version="3.2.6" comp="ge"/>
  </requires>

  <post-install>

    <process command="./wsh.php --api=setStyle --style=STYLE/@APPNAME@/@APPNAME@.sty">
      <label lang="en">Set blue style</label>
    </process>
  </post-install>

  <post-upgrade>
      <process command="./wsh.php --api=setStyle --style=STYLE/@APPNAME@/@APPNAME@.sty">
        <label lang="en">Reset blue style</label>
      </process>
  </post-upgrade>


</module>
